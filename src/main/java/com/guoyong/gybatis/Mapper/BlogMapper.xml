<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.guoyong.gybatis.Mapper.BlogMapper">
    <select id="selectBlog" resultType="com.guoyong.gybatis.Domain.Blog">
    select * from Blog where id = #{id}
  </select>
    <select id="selectBlogByMap" resultType="com.guoyong.gybatis.Domain.Blog">
    select * from Blog where name = #{name}
  </select>
</mapper>